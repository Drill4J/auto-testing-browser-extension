{
  "manifest_version": 2,
  "name": "Drill4J Auto Testing Extension",
  "version": "0.7.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "default-logo.png",
      "128": "large-logo.png"
    }
  },
  "permissions": ["tabs", "webNavigation", "webRequest", "webRequestBlocking", "activeTab", "storage", "debugger", "*://*/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}